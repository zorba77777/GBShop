# platform :ios, '9.0'

target 't.sasin' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for t.sasin
    pod 'Alamofire'
    pod 'OHHTTPStubs/Swift'
    pod 'Fabric'
    pod 'Crashlytics'
    
    target 't.sasinTests' do
        inherit! :search_paths
        # Pods for testing
        pod 'Alamofire'
        pod 'OHHTTPStubs/Swift'
        pod 'Fabric'
        pod 'Crashlytics'
    end
    
    target 't.sasinUITests' do
        inherit! :search_paths
        # Pods for testing
        pod 'Alamofire'
        pod 'OHHTTPStubs/Swift'
        pod 'Fabric'
        pod 'Crashlytics'
    end
    
end
